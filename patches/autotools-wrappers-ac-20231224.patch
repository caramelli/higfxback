--- autotools-wrappers-ac-20231224.orig/ac-wrapper.sh
+++ autotools-wrappers-ac-20231224/ac-wrapper.sh
@@ -213,6 +213,11 @@
 #
 export WANT_AUTOCONF="${binary_ver}"
 
+if test -f configure && ! test -x configure; then
+	sleep 1
+	chmod +x configure
+fi
+
 #
 # Now try to run the binary
 #
--- autotools-wrappers-ac-20231224.orig/autoconf-2.60/bin/Makefile.in
+++ autotools-wrappers-ac-20231224/autoconf-2.60/bin/Makefile.in
@@ -477,6 +477,8 @@
 	  rm -f forbidden.log; \
 	fi
 
+$(top_builddir)/bin/autom4te: autom4te
+
 # autoconf is written in M4sh.
 # FIXME: this target should depend on the frozen files below lib/m4sugar,
 # otherwise autom4te may pick up a frozen m4sh.m4f from an earlier
--- autotools-wrappers-ac-20231224.orig/autoconf-2.61/bin/Makefile.in
+++ autotools-wrappers-ac-20231224/autoconf-2.61/bin/Makefile.in
@@ -494,6 +494,8 @@
 	  rm -f forbidden.log; \
 	fi
 
+$(top_builddir)/bin/autom4te: autom4te
+
 # autoconf is written in M4sh.
 # FIXME: this target should depend on the frozen files below lib/m4sugar,
 # otherwise autom4te may pick up a frozen m4sh.m4f from an earlier
--- autotools-wrappers-ac-20231224.orig/autoconf-2.66/lib/autoconf/general.m4
+++ autotools-wrappers-ac-20231224/autoconf-2.66/lib/autoconf/general.m4
@@ -224,14 +224,20 @@
 # The solution is to require AC_INIT in each of these macros.  AC_INIT
 # has the needed magic so that it can't be expanded twice.
 
-
+# _AC_INIT_LITERAL(STRING)
+# ------------------------
+m4_define([_AC_INIT_LITERAL],
+m4_if(m4_index([$1], [@S|@]),
+  [m4_warn([syntax],[AC_INIT: not a literal: $1])],
+  m4_if(m4_index(m4_translit([[$1]], [\`], [$]), [$]), [],
+    [m4_warn([syntax],[AC_INIT: not a literal: $1])])))
 
 # _AC_INIT_PACKAGE(PACKAGE-NAME, VERSION, BUG-REPORT, [TARNAME], [URL])
 # ---------------------------------------------------------------------
 m4_define([_AC_INIT_PACKAGE],
-[AS_LITERAL_IF([$1], [], [m4_warn([syntax], [AC_INIT: not a literal: $1])])
-AS_LITERAL_IF([$2], [],  [m4_warn([syntax], [AC_INIT: not a literal: $2])])
-AS_LITERAL_IF([$3], [],  [m4_warn([syntax], [AC_INIT: not a literal: $3])])
+[_AC_INIT_LITERAL([$1])
+_AC_INIT_LITERAL([$2])
+_AC_INIT_LITERAL([$3])
 m4_ifndef([AC_PACKAGE_NAME],
 	  [m4_define([AC_PACKAGE_NAME],     [$1])])
 m4_ifndef([AC_PACKAGE_TARNAME],
--- autotools-wrappers-ac-20231224.orig/autoconf-2.67/lib/autoconf/general.m4
+++ autotools-wrappers-ac-20231224/autoconf-2.67/lib/autoconf/general.m4
@@ -239,7 +239,7 @@
 m4_define([_AC_INIT_PACKAGE],
 [_AC_INIT_LITERAL([$1])
 _AC_INIT_LITERAL([$2])
-AS_LITERAL_IF([$3], [],  [m4_warn([syntax], [AC_INIT: not a literal: $3])])
+_AC_INIT_LITERAL([$3])
 m4_ifndef([AC_PACKAGE_NAME],
 	  [m4_define([AC_PACKAGE_NAME],     [$1])])
 m4_ifndef([AC_PACKAGE_TARNAME],

